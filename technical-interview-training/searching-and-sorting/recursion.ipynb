    "\"\"\"Implement a function recursivly to get the desired Fibonacci sequence value.\"\"\"\n",
    "\n",
    "def get_fib(position):\n",
    "    if position < 2:\n",
    "        return position\n",
    "    return get_fib(position-1) + get_fib(position-2)\n",
    "\n",
    "# Test cases\n",
    "print get_fib(9)\n",
    "print get_fib(11)\n",
    "print get_fib(0)"
   ]
  }
